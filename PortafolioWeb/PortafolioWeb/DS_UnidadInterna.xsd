          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="ResumenFuncionariosTableAdapter" GeneratorDataComponentClassName="ResumenFuncionariosTableAdapter" Name="ResumenFuncionarios" UserDataComponentName="ResumenFuncionariosTableAdapter">
